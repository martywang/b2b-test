{
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"System": "Information",
			"Microsoft": "Information"
		}
	},
	"vcap": {
			"services": {
				"mongodb-dev": [
					{
						"name": "mongo", // name must be specified for eureka to properly run locally
						// local mongo instance
						"credentials": {
							"database": "fnol",
							"password": "",
							"servers": [
								"localhost:27017"
							],
							"uri": "mongodb://localhost:27017/fnol",
							"username": ""
						}
					}
				],
				"rabbitmq-dev": [
					{
						"name": "rabbitMq", // name must be specified for steeltoe to locate the rabbit service and for eureka to properly run locally
						"credentials": {
							// local"
							"uri": "amqp://guest:guest@localhost:5672/"
							
						}
					}
				]

			}
	}
}
